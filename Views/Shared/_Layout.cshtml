<!DOCTYPE html>
<html lang="en">

<head>

  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">

     <title>Airim</title>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/css/site.css" />
    <link href="~/css/simple-sidebar.css" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Roboto:100,300,400,500,700,900" rel="stylesheet">
    <link href='https://cdn.jsdelivr.net/npm/@@mdi/font@4.x/css/materialdesignicons.min.css' rel="stylesheet">
    <link href='https://cdn.jsdelivr.net/npm/vuetify@2.x/dist/vuetify.min.css' rel="stylesheet">
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.16.0/umd/popper.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vue@2.x/dist/vue.js"></script>
    <script src="https://cdn.jsdelivr.net/npm/vuetify@2.x/dist/vuetify.js"></script>
    <script src="https://unpkg.com/axios/dist/axios.min.js"></script>

    
    
  <!-- Custom styles for this template -->
 

</head>

<body>

  <v-app id="app">
    <v-navigation-drawer app v-model="drawer" :mini-variant.sync="mini" permanent>
      <v-list-item class="px-2">
        <v-list-item-avatar size="50">
          <v-img src="/images/123.jpg"></v-img>
        </v-list-item-avatar>

        <v-list-item-title>AIRIM</v-list-item-title>

        <v-btn icon @@click.stop="mini = !mini">
          <v-icon>mdi-chevron-left</v-icon>
        </v-btn>
      </v-list-item>

      <v-divider></v-divider>

      <v-list dense>
        <v-list-item v-for="item in menuitems" :key="item.title" @@click="item.method" link>
          <v-list-item-icon>
            <v-icon>{{ item.icon }}</v-icon>
          </v-list-item-icon>

          <v-list-item-content>
            <v-list-item-title>{{ item.title }}</v-list-item-title>
          </v-list-item-content>
        </v-list-item>
      </v-list>
    </v-navigation-drawer>
  
    <v-app-bar app>
      <v-row justify="center">
        <v-col class="grey lighten-1 text-center white--text" cols="12">
            <strong>AIRIM</strong>
      </v-col>
    </v-row>
    </v-app-bar>
  
    <!-- Sizes your content based upon application components -->
    <v-main>
  
      <!-- Provides the application the proper gutter -->
      <v-container fluid>
          @RenderBody()
              <!--<div class="container">
       <main role="main" class="pb-3">
            @RenderBody()
        </main>-->
    </div>

        <!-- If using vue-router -->
        <!--<router-view></router-view>-->
      </v-container>
    </v-main>
  
    <!-- <v-footer app>
      <v-row justify="center">
        <v-col cols=2></v-col>
        <v-col class="grey lighten-1 text-center white--text" cols="10">
            {{ new Date().getFullYear() }} — <strong>CELENA</strong>
      </v-col>
    </v-row>
     </v-footer>-->
 </v-app>
<script src="~/js/app.js"></script>
</body>

</html>
